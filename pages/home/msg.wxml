<!--pages/home/msg.wxml-->
<view class='container'>
  <view class='item' wx:for='{{msgs}}' wx:key='{{index}}'>
    <view class='content'>{{item.content}}</view>
    <view class='date'>{{item.time}}</view>
  </view>
  <view class='view-more' bindtap='onViewMoreTap'>{{hasMore?'查看更多':'没有更多了'}}</view>
</view>
